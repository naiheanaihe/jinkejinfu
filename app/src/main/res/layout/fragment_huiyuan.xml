            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="50dp"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="VIP1："

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/vip_huang"
                android:text="30元/终生"
                android:textSize="16sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
            <Button
                android:id="@+id/zhuce2"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:text="注册"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:background="@drawable/denglu_biankuang"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/text2_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="自动晋级条件："
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"/>
            <TextView
                android:id="@+id/text2_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/tolbar_color"
                android:text="累计业绩500元"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@id/text2_1"
                android:layout_marginTop="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/tolbar_color"
                android:text="推荐用户50人"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="7dp"
                android:layout_below="@id/text2_2"
                android:layout_toRightOf="@id/text2_1"/>
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textColor="@color/tolbar_color"
